#include <stdio.h> // 1 증가연산자
int main() {
  int a=5, b=6, c=7;
  c(a++, ++b, a+b);
  printf("%d, %d, %d\n ", a,b,c);
return 0;
}

#include <stdio.h> // 2 조건연산자, 단락평가
